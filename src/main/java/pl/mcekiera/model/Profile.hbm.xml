<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pl.mcekiera.model.Profile" table="profiles" schema="torrent">
            <id name="id" column="id"/>
            <property name="minRating" column="minRating"/>
            <property name="whitelist" column="whitelist"/>
            <property name="blacklist" column="blacklist"/>
    </class>
</hibernate-mapping>